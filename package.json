    "scss:prod": "node-sass --output-style compressed -o public/css app/scss/main.scss",
    "server:build": "babel server --out-dir build --presets=es2015 --ignore=node_modules,compiled --source-maps inline",
    "test:react": "mocha --compilers js:babel-core/register --require ./test/components/test_helper.js --recursive ./test/components",
    "test:react:watch": "npm run test:react -- --watch"
