  "scripts": {
    "test": "npm run compile-for-test && npm run test:react && ./node_modules/.bin/mocha --reporter spec",
    "webpack": "webpack --progress --colors --watch",
    "start": "nodemon server/server.js",
    "test2": "mocha tests/.setup.js tests/tester.js",
    "mysql": "mysql -h rds-mysql-discollectdb.c0eor5cjwn0u.us-west-1.rds.amazonaws.com -P 3306 -u jordan -p",
